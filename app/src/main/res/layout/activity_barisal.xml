<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#2273ff"
    tools:context=".BarisalActivity">


    <Button
        android:id="@+id/kuakataseabeachId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Kuakata Sea Beach"
        android:textSize="30sp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/guthiamosqueId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Guthia Mosque"
        android:textSize="30sp"
        android:textStyle="bold"
        />
    <Button
        android:id="@+id/durgasagorId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Durga Sagor"
        android:textSize="30sp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/piarabazarId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Floating Guava Market"
        android:textSize="30sp"
        android:textStyle="bold"
        />


</LinearLayout>



