<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#2273ff"
    tools:context=".MymenshignActivity">


    <Button
        android:id="@+id/bangladeshagriculturaluniversityId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Bangladesh Agricultural University"
        android:textSize="30sp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/botanicalgardenId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Botanical Garden"
        android:textSize="30sp"
        android:textStyle="bold"
        />
    <Button
        android:id="@+id/museummymenshignId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Museum Mymenshign"
        android:textSize="30sp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/nuzrulmemorialcenterId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nuzrul Memorial Center"
        android:textSize="30sp"
        android:textStyle="bold"
        />



</LinearLayout>



